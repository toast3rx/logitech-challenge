{
  "name": "logitech",
  "displayName": "logitech",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "logitech.identifiers",
      "title": "Iterate through identifiers"
    },
    {
      "command": "logitech.definition",
      "title": "Go to definition"
    }],
    "configuration": {
      "title": "Logitech",
      "properties": {
        "Logitech.MX\u0020Master\u00203\u0008S.forwardButton": {
          "type": "string",
          "default": "Iterate through iterators forward",
          "enum": [
            "Iterate through iterators forward",
            "Iterate through iterators backward"
          ],
          "description": "Configure action for the Forward Button"
        },
        "Logitech.MX\u0020Master\u00203\u0008S.backButton": {
          "type": "string",
          "default": "Iterate through iterators backward",
          "enum": [
            "Iterate through iterators forward",
            "Iterate through iterators backward"
          ],
          "description": "Configure action for the Back Button"
        },
        "Logitech.MX\u0020Master\u00203\u0008S.middleButton": {
          "type": "string",
          "default": "",
          "enum": [
            "Go to definition",
            "Option 2",
            "Option 3"
          ],
          "description": "Configure action for the Middle Button"
        },
        "Logitech.MX\u0020Master\u00203\u0008S.topButton": {
          "type": "string",
          "default": "",
          "enum": [
            "Go to definition",
            "Option 2",
            "Option 3"
          ],
          "description": "Configure action for the Top Button"
        },
        "Logitech.MX\u0020Master\u00203\u0008S.thumbButton": {
          "type": "string",
          "default": "",
          "enum": [
            "Go to definition",
            "Option 2",
            "Option 3"
          ],
          "description": "Configure action for the Thumb Button (Gestures)"
        },
        "Logitech.MX\u0020Master\u00203\u0008S.thumbWheel": {
          "type": "string",
          "default": "",
          "enum": [
            "Iterate through iterators forward",
            "Iterate through iterators backward"
          ],
          "description": "Configure action for the Thumb Wheel"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "fs-extra": "^10.1.0",
    "logi-plugin-sdk": "file:../logi-plugin-sdk-js",
    "open": "^9.1.0"
  }
}
